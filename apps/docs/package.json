{
  "private": true,
  "name": "docs",
  "description": "",
  "license": "",
  "version": "0.0.1",
  "scripts": {
    "build": "yarn build:remix && yarn build:css",
    "build:remix": "remix build",
    "build:css": "tailwindcss -i ./styles/index.css -o ./app/styles.css",
    "dev": "concurrently \"yarn dev:remix\" \"yarn dev:css\"",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss -i ./styles/index.css -o ./app/styles.css --watch",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@remix-run/vercel": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "remix-validated-form": "*"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.5.1",
    "tailwindcss": "3.0.7",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
