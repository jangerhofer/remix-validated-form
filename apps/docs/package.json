  "version": "0.0.1",
  "scripts": {
    "build": "yarn build:remix && yarn build:css",
    "build:remix": "remix build",
    "build:css": "postcss ./styles/index.css --output app/styles/index.css --env production",
    "dev": "concurrently \"yarn dev:remix\" \"yarn dev:css\"",
    "dev:remix": "remix dev",
    "dev:css": "TAILWIND_MODE=watch postcss ./styles/index.css --output app/styles/index.css --watch",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@remix-run/vercel": "^1.1.1",
    "classnames": "^2.3.1",
    "framer-motion": "^5.5.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "remix-validated-form": "*"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.0",
    "classnames": "^2.3.1",
    "concurrently": "^6.5.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "tailwindcss": "3.0.7",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
